          <hr style="width:100%;"> 

          <div class="total_carrito">Total:</div>
          <div class="precio_carrito">$700</div>
          
          <button style="margin-left:auto; margin-top:10%; margin-right:2%; margin-bottom:2%;" type="button" class="btn btn-success">Comprar Ahora</button>
           
        </div>
      </div>  
      
        
