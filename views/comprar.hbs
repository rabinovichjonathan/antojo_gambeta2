<main class="main">
        <div class="titulo_sobre_nosotros">
            {{title}}
        </div>
       <hr>
       <div class="contenedor_gral_comprar">
        
        <div class="contenedor_listado_productos_comprar">
             {{#each products }} 
              <div class="imagen_producto_comprar">
                {{#if foto_producto}}  
                  <a href="producto_detalle?producto_id={{producto_id}}"><img src="imagenes/{{foto_producto}}"></a>
              {{^}}
                  <a href="producto_detalle?producto_id={{producto_id}}"><img src="imagenes/logo_antojo_gambeta.jpeg"></a>
              {{/if}}
               
              </div>
              <div class="contenedor_titulo_copete_precio_botton">
                <div class="titulo_producto_comprar">
                  {{producto}}
                </div>
                <div class="copete_producto_comprar">
                  {{descripcion}}
                </div>
                <div class="precio_producto">${{precio}}</div>
                <label style="font-size: 12px; margin-top:4%;"for="unidades">Cantidad de unidades:</label><br>
                <input style="width:10%; height:5%; margin-top:3%; margin-left:2%;" type="number" name="unidades">
                <button type="button" class="btn btn-danger" style="margin-left: auto; margin-top:3%;">Agregar</button>
              </div>
              
              <hr style="width:100%;">
              {{/each}}
           
        </div>
        
            
           
          
          
          
         
       
       
        <div class="contenedor_carrito">
          <div class="titulo_mis_productos">
            Mis Productos
          </div>
           <div style="font-weight:bold;"class="total_carrito">x1</div> 
           <div class="total_carrito">Canastita de berenjena</div>
           <div style="margin-right: 2%;" class="precio_carrito">$700</div>
           
